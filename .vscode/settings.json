{
  "editor.formatOnSave": false,
  "editor.formatOnType": false,
  "editor.formatOnPaste": false,
  "workbench.statusBar.feedback.visible": false,
  "typescript.format.enable": false,
  "typescript.disableAutomaticTypeAcquisition": true,
  "typescript.check.npmIsInstalled": false,
  "typescript.surveys.enabled": false,
  "typescript.tsdk": "node_modules/typescript/lib",
  "typescript.tsc.autoDetect": "off",
  "javascript.format.enable": false,
  "eslint.enable": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "eslint.run": "onSave",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "search.exclude": {
    "**/node_modules/": true,
    ".devcontainer/": true,
    ".vscode/": true,
    "coverage/": true,
    "packages/*/build/": true,
    "packages/*/*/build/": true,
    "tmp/": true
  },
  "search.followSymlinks": false,
  "peacock.color": "#0070d7",
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#0b8aff",
    "activityBar.activeBorder": "#ffbfe0",
    "activityBar.background": "#0b8aff",
    "activityBar.foreground": "#e7e7e7",
    "activityBar.inactiveForeground": "#e7e7e799",
    "activityBarBadge.background": "#ffbfe0",
    "activityBarBadge.foreground": "#15202b",
    "statusBar.background": "#0070d7",
    "statusBar.border": "#0070d7",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#0b8aff",
    "titleBar.activeBackground": "#0070d7",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.border": "#0070d7",
    "titleBar.inactiveBackground": "#0070d799",
    "titleBar.inactiveForeground": "#e7e7e799"
  }
}
